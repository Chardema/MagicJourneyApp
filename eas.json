{
  "cli": {
    "version": ">= 0.47.0"
  },
  "build": {
    "development": {
      "ios": {
        "credentialsSource": "remote",
        "distribution": "internal",
        "developmentClient": true
      },
      "android": {
        "credentialsSource": "remote",
        "distribution": "internal",
        "developmentClient": true
      }
    },
    "production": {
      "ios": {
        "credentialsSource": "remote",
        "distribution": "store"
      },
      "android": {
        "credentialsSource": "remote",
        "buildType": "apk"
      }
    },
    "testflight": {
      "ios": {
        "credentialsSource": "remote",
        "distribution": "store"
      }
    }
  }
}
